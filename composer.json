{
    "name": "dkrizan/magiogo-streamer",
    "description": "Scrap stream url, record tv channels and much more.",
    "keywords": [
        "php",
        "slim-micro-framework",
        "slim4",
        "slim",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Krizan",
            "email": "danyelkrizan@gmail.com",
            "homepage": "https://github.com/dkrizan"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^0.6.0",
        "selective/basepath": "^1.0",
        "symfony/cache": "^4.4",
        "predis/predis": "^1.1",
        "php-di/slim-bridge": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "hassankhan/config": "^2.2",
        "ext-json": "*",
        "symfony/yaml": "^5.1",
        "monolog/monolog": "^2.1",
        "doctrine/orm": "^2.8",
        "symfony/uid": "^5.2",
        "ramsey/uuid-doctrine": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Libs\\": "libs/",
            "Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
